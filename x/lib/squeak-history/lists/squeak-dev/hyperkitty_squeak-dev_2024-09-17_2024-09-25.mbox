Item was changed:
  ----- Method: ClipboardTest>>testClipboardTextNotifyWith (in category 'test=
s') -----
  testClipboardTextNotifyWith
 =20
  	| uuidString clipboard subscriber notifiedText notifiedSource |
  	uuidString :=3D UUID new asString.
  	clipboard :=3D Clipboard new.
  	subscriber :=3D [ :text :source |=20
  		notifiedText :=3D text.
  		notifiedSource :=3D source ].
  	clipboard=20
  		when: #contentChanged
  		send: #value:value:
  		to: subscriber.
  	clipboard clipboardText: uuidString.
  	self
+ 		assert: uuidString equals: notifiedText asString;
- 		assert: uuidString equals: notifiedText;
  		assert: nil equals: notifiedSource.
  	clipboard clipboardText: uuidString reversed notifyWith: self.
  	self
+ 		assert: uuidString reversed equals: notifiedText asString;
- 		assert: uuidString reversed equals: notifiedText;
  		assert: self =3D=3D notifiedSource.
  	clipboard removeActionsWithReceiver: subscriber.
  	clipboard clipboardText: uuidString.
  	self
+ 		assert: uuidString reversed equals: notifiedText asString;
- 		assert: uuidString reversed equals: notifiedText;
  		assert: self =3D=3D notifiedSource!


