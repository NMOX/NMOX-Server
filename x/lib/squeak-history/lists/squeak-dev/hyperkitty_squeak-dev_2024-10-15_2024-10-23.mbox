+ innermostRangeForSourceIndex: sourceIndex in: ranges
+ 	^ranges
+ 		inject: (ranges detect: [:range| range includes: sourceIndex])
+ 		into: [:best :next|
+ 			((next includes: sourceIndex)
+ 			 and: [best first < next first
