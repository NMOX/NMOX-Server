  ----- Method: MorphicToolBuilder>>buildPluggableButton: (in category 'widge=
ts required') -----
  buildPluggableButton: aSpec
  	| widget label state action enabled |
  	label :=3D aSpec label.
  	state :=3D aSpec state.
  	action :=3D aSpec action.
  	widget :=3D self buttonClass on: aSpec model
+ 				getState: ((state isSymbol or: [state isClosure]) ifTrue: [state])
- 				getState: (state isSymbol ifTrue:[state])
  				action: nil
  				label: (label isSymbol ifTrue:[label]).
  	widget style: aSpec style.
  	aSpec changeLabelWhen
  		ifNotNil: [ :event | widget whenChanged: event update: aSpec label].
  	self register: widget id: aSpec name.
  	enabled :=3D aSpec enabled.
  	enabled isSymbol
  		ifTrue:[widget getEnabledSelector: enabled]
  		ifFalse:[widget enabled:enabled].
  	widget action: action.
  	aSpec color isColor
  		ifTrue: [widget offColor: aSpec color]
  		ifFalse: [widget getColorSelector: aSpec color].
  	self buildHelpFor: widget spec: aSpec.=20
  	(label isSymbol or:[label =3D=3D nil]) ifFalse:[widget label: label].
  	self setFrame: aSpec frame in: widget.
  	self setLayoutHintsFor: widget spec: aSpec.
  	parent ifNotNil:[self add: widget to: parent].
  	^widget!


